{% extends 'FOSUserBundle::layout.html.twig' %}

{% set page_title = 'resetting.request.submit' | trans({}, 'FOSUserBundle') %}

{% block content %}
<p>{{ 'resetting.check_email' | trans({ '%email%': '<strong>' ~ email ~ '</strong>' }, 'FOSUserBundle') | raw }}</p>

<div class="btn-group">
    <a href="{{ path('fos_user_security_login') }}" class="btn btn-default">
        <i class="fa fa-chevron-circle-left"></i> {{ 'Back' | trans }}
    </a>
</div>
{% endblock %}
